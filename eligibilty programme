#include<iostream>


int main()
{
	int age;

	cout<<"Enter your age: ";
	cin>>age;
  
	//>=13 and <=19
	if(age>=13 && age<=19)
	{
		cout<<"Person is an adult"<<endl;
	}
	else
	{
		cout<<"Person is not a not an adult "<<endl;
	}


	
	if(age>=18)
	{
		cout<<"Personl is eligible for voting"<<endl;
	}
	else
	{
		cout<<"Person is not eligible for voating"<<endl;
	}

	return 0;
}
