int main()
{
     int i=2;
     switch (i)
     {
        case 1:
           printf("pizza  ");
        case 2:
           printf("pasta");
        case 3:
           printf("pita ");
        case 4:
           printf("pesto ");
        default:
           printf("pouri ");
     }
    return 0;
}
